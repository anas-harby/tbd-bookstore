<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.BorderPane?>
<fx:root type="com.jfoenix.controls.JFXTreeTableView" fx:id="root" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" >
    <columns>
        <JFXTreeTableColumn fx:id="isbnCol" text="ISBN" minWidth='100' resizable="false"/>
        <JFXTreeTableColumn fx:id="titleCol" text="Title" minWidth='100' resizable="false"/>
        <JFXTreeTableColumn fx:id="authorCol" text="Author" minWidth='100' resizable="false"/>
        <JFXTreeTableColumn fx:id="yearCol" text="Year" minWidth='100' resizable="false"/>
        <JFXTreeTableColumn fx:id="genreCol" text="Genre" minWidth='100' resizable="false"/>
        <JFXTreeTableColumn fx:id="publisherCol" text="Publisher" minWidth='100' resizable="false"/>
        <JFXTreeTableColumn fx:id="quantityCol" text="Quantity" minWidth='100' resizable="false"/>
        <JFXTreeTableColumn fx:id="cartBtnCol" sortable="false" />
    </columns>
    <BorderPane.margin>
        <Insets bottom="10.0" right="10.0" top="20.0" />
    </BorderPane.margin>
</fx:root>
